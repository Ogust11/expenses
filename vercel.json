// vercel.json
{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/expenses",
      "destination": "/api/expenses.js"
    },
    {
      "source": "/api/expenses/(.*)",
      "destination": "/api/expenses.js"
    }
  ],
  "builds": [
    {
      "src": "api/expenses.js",
      "use": "@vercel/node"
    }
  ]
}
